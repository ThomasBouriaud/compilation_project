fonction main:
	-- on peut changer la couleur de la tortue
	-- il y a deux couleurs ( carapace et motif )

	-- les couleurs sont donn ées en hexad é cimal au format # RRGGBB
	-- comme en CSS

	couleur carapace #aBcDeF
	couleur motif #282828

	couleur #424242 -- é quivalent à 'couleur carapace #424242 '

fin fonction
